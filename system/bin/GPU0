#!/system/bin/sh
clear
echo  [!] sc by @bintang774
echo ===================================
echo  Turning Off GPU Performance MODE...
echo ===================================
sleep 1
echo  Wait...
sleep 2
setprop sys.hwc.gpu_perf_mode 0
setprop debug.hwui.renderer skiagl
setprop debug.performance.tuning 0
setprop hw3d.force 0
echo DONE
